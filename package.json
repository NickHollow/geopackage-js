{
  "name": "@ngageoint/geopackage",
  "version": "2.0.7",
  "description": "GeoPackage JavaScript Library",
  "keywords": [
    "NGA",
    "geopackage"
  ],
  "license": "MIT",
  "author": {
    "name": "NGA",
    "url": "https://www.nga.mil"
  },
  "contributors": [
    "Daniel Barela <daniel.barela@gmail.com>"
  ],
  "homepage": "http://ngageoint.github.io/geopackage-js/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngageoint/geopackage-js.git"
  },
  "bugs": {
    "url": "https://github.com/ngageoint/geopackage-js/issues"
  },
  "bin": "./cli",
  "dependencies": {
    "@mapbox/vector-tile": "^1.3.1",
    "@turf/bbox": "^6.0.1",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/boolean-within": "^6.0.1",
    "@turf/circle": "^6.0.1",
    "@turf/distance": "^6.0.1",
    "@turf/intersect": "^6.1.2",
    "@turf/line-intersect": "^6.0.2",
    "@turf/point-to-line-distance": "^6.0.0",
    "@turf/polygon-to-line": "^6.0.3",
    "better-sqlite3": "5.4.0",
    "d3-geo": "^1.10.0",
    "file-type": "8.1.0",
    "geojson-vt": "3.2.1",
    "pbf": "^3.1.0",
    "proj4": "2.5.0",
    "pureimage": "0.1.6",
    "rbush": "2.0.2",
    "reproject": "1.2.2",
    "sql.js": "https://github.com/danielbarela/sql.js.git",
    "vt-pbf": "^3.1.1",
    "wkx": "file:vendor/wkx-esm"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.3",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babelify": "^10.0.0",
    "canvas-compare": "^3.0.0",
    "chai": "^3.5.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.2.0",
    "foodoc": "0.0.9",
    "fs-extra": "^2.1.2",
    "jsdoc": "~3.5.5",
    "mocha": "^6.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^10.0.2",
    "nyc": "^13.1.0",
    "rollup": "^1.4.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.0",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5",
    "webworkify": "^1.1.0",
    "xhr-mock": "^2.4.1"
  },
  "optionalDependencies": {
    "chalk": "^1.1.3",
    "inquirer": "^1.0.2"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.es.js",
  "files": [
    "index.js",
    "static/geopackage.js",
    "cli",
    "lib",
    "converters",
    "dist"
  ],
  "browser": {
    "sqlite3": false
  },
  "nyc": {
    "sourceMap": false,
    "temp-dir": "./.test_run",
    "report-dir": "./coverage",
    "reporter": [
      "html",
      "text"
    ],
    "require": [
      "@babel/register"
    ],
    "exclude": [
      "vendor/**/*",
      "test/**/*",
      "lib/tiles/creator/canvas.js",
      "lib/tiles/creator/projectTile.js"
    ]
  },
  "scripts": {
    "prepare-it": "cross-env NODE_OPTIONS=--max-old-space-size=8192 rollup -c rollup.config.js",
    "clean": "rm -rf ./.test_run; rm -rf ./.nyc_output; rm -rf ./coverage",
    "test-sqlnode": "cross-env NODE_ENV=test nyc --no-clean mocha --exit --allow-uncaught",
    "test-sqljs": "cross-env NODE_ENV=test FORCE_SQLJS=true nyc --no-clean mocha --allow-uncaught",
    "test": "npm run clean && npm run test-sqlnode && npm run test-sqljs && npm run report",
    "testb": "npm run clean && cross-env NODE_ENV=test nyc --reporter=html mocha",
    "report": "nyc report",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "predoc": "rm -rf docs/jsdoc/*; ./docs/tutorials/createTutorials.js",
    "doc": "jsdoc --verbose -c jsdoc.conf",
    "prepublishold": "mkdir dist; browserify -t browserify-css index.js --standalone geopackage --exclude sqlite3 --exclude inquirer -o dist/geopackage.js && uglifyjs dist/geopackage.js > dist/geopackage.min.js",
    "typescript-build": "rm -rf dist && tsc",
    "typescript-browser": "rm -rf dist.browser && tsc -p tsconfig.browser.json && cross-env NODE_OPTIONS=--max-old-space-size=8192 rollup -c rollup.tsc.config.js",
    "rollup-tsc": "rm -rf dist && cross-env NODE_OPTIONS=--max-old-space-size=8192 rollup -c rollup.tsc.config.js"
  }
}
